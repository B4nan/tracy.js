{
  "name": "tracy.js",
  "version": "0.4.3",
  "description": "node.js debugger module that displays stack traces in readable html",
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/B4nan/tracy.js.git"
  },
  "keywords": [
    "debugger",
    "debugging",
    "error",
    "stack",
    "trace",
    "node",
    "es6",
    "typescript",
    "ts",
    "readable",
    "friendly",
    "html",
    "tracy"
  ],
  "author": "Martin Ad√°mek",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/B4nan/tracy.js/issues"
  },
  "homepage": "https://github.com/B4nan/tracy.js#readme",
  "dependencies": {
    "lodash.merge": "^4.6.1",
    "typescript": "^2.9.2"
  },
  "devDependencies": {
    "@types/jest": "^23.3.1",
    "@types/lodash": "^4.14.112",
    "@types/node": "^10.5.4",
    "coveralls": "^3.0.2",
    "git-pre-commit": "^2.1.4",
    "jest": "^23.4.2",
    "ts-jest": "^23.0.1",
    "ts-node": "^7.0.0",
    "tslint": "^5.11.0"
  },
  "jest": {
    "verbose": true,
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/tests/.*\\.(test|spec))\\.(js|ts)$",
    "testEnvironment": "node",
    "moduleFileExtensions": [
      "ts",
      "js",
      "json"
    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "lib/**/*.ts"
    ]
  },
  "scripts": {
    "build": "tsc",
    "precommit": "yarn lint",
    "test": "node node_modules/.bin/jest",
    "coveralls": "cat ./coverage/lcov.info | node node_modules/.bin/coveralls",
    "lint": "tslint -c tslint.json -p tsconfig.json"
  }
}
